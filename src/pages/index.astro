---
import Layout from '../layouts/Layout.astro';
import MusicGrid from '../components/MusicGrid.astro';
---

<Layout title="Music - My Dashboard">
  <main id="main-content" class="max-w-7xl">
    <header class="mb-8">
      <h1 class="text-4xl font-bold mb-2">Music Ideas</h1>
      <p class="text-gray-400">
        Track your daily music ideas and compositions
      </p>
    </header>

    <div class="music-container">
      <MusicGrid />
    </div>
  </main>
</Layout>
